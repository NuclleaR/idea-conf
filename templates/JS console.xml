<templateSet group="JS console">
  <template name="loc" value="console.group('$METHOD_NAME$');&#10;console.groupEnd();" description="console.group" toReformat="false" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="log" value="console.log($END$);" description="console.log" toReformat="false" toShortenFQNames="true" />
  <template name="logo" value="console.log('%O', $END$)" description="console.log('%O')" toReformat="false" toShortenFQNames="true" />
  <template name="lom" value="console.log('$CLASS$ @ $FUNC$:',$END$)" description="Log method" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="FUNC" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="loo" value="console.log('$VAR$:', $VAR$);" description="Log var" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
</templateSet>